{
    "name": "winatabayu00/package-based",
    "description": "Laravel base package to help with modular and service-based project development.",
    "type": "library",
    "license": "MIT",
    "version": "1.0.9",
    "authors": [
        {
            "name": "Winata Bayu",
            "email": "winatabayu01@email.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-intl": "*",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "Winata\\PackageBased\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Winata\\PackageBased\\BaseServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
